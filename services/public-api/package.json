{
    "name": "public-api",
    "version": "0.0.0",
    "private": true,
    "main": "src/index.js",
    "dependencies": {
      "axios": "^0.21.1",
      "body-parser": "^1.19.0",
      "cookie-parser": "^1.4.5",
      "cors": "^2.8.5",
      "express": "^4.17.1",
      "express-async-handler": "^1.1.4",
      "express-jwt": "^6.0.0",
      "ioredis": "^4.22.0",
      "jsonwebtoken": "^8.5.1",
      "morgan": "^1.10.0",
      "swagger-jsdoc": "^6.0.0",
      "swagger-ui-express": "^4.1.6",
      "ts-node": "^9.1.1",
      "typescript": "^4.1.3",
      "qs": "^6.9.6",
      "uuid": "^8.3.2",
      "valid-url": "^1.0.9",
      "@openapitools/openapi-generator-cli": "^2.1.16",
      "@types/body-parser": "^1.19.0",
      "@types/cors": "^2.8.9",
      "@types/cookie-parser": "^1.4.2",
      "@types/express": "^4.17.10",
      "@types/express-jwt": "^6.0.0",
      "@types/ioredis": "^4.19.4",
      "@types/jsonwebtoken": "^8.5.0",
      "@types/morgan": "^1.9.2",
      "@types/swagger-jsdoc": "^6.0.0",
      "@types/swagger-ui-express": "^4.1.2",
      "@types/qs": "^6.9.5",
      "@types/uuid": "^8.3.0",
      "@types/valid-url": "^1.0.3"
    },
    "devDependencies": {
      "nodemon": "^2.0.7"
    },
    "scripts": {
      "generate-api-client-movies": "openapi-generator-cli generate -g typescript-axios -i apis/movies.json -o src/api/movies/",
      "generate-api-client-relations": "openapi-generator-cli generate -g typescript-axios -i apis/relations.json -o src/api/relations/",
      "generate-api-client-reviews": "openapi-generator-cli generate -g typescript-axios -i apis/reviews.json -o src/api/reviews/",
      "generate-api-client-users": "openapi-generator-cli generate -g typescript-axios -i apis/users.json -o src/api/users/",
      "generate-api-schema-public": "swagger-jsdoc -d openapi-definition.json src/*.ts src/services/*.ts -o generated/openapi.json",
      "build": "tsc --project ./",
      "start": "ts-node src/index.ts",
      "start-dev": "nodemon src/index.ts"
    }
}
